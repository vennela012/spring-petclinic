pipeline{
    agent any
    stages{
        stage ('vcs'){
            steps{
                git branch: 'vens' 
                git url: 'https://github.com/vennela012/spring-petclinic.git'
            }
            stage ('build'){
                steps{
                    sh "mvn package"
                }
                stage('archive results'){
                    steps{
                        junits '**/surefile-reports/*.xml'


                    }
                }

            }
        }
    }
}